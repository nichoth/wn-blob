{
  "name": "@nichoth/wn-blob",
  "description": "Save blobs to webnative",
  "directories": {
    "test": "test"
  },
  "type": "module",
  "version": "0.0.0",
  "main": "dist/index.js",
  "browser": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "tsc -w",
    "prebuild": "rm -rf dist/*",
    "build": "tsc",
    "test": "browserify -d -p esmify test/index.js | tape-run --browser=\"chrome\" --render=\"tap-spec\""
  },
  "dependencies": {
  },
  "devDependencies": {
    "browserify": "^17.0.0",
    "esmify": "github:nichoth/esmify",
    "tap-spec": "^5.0.0",
    "tape": "^5.3.1",
    "tape-run": "^9.0.0",
    "webnative": "^0.28.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nichoth/wn-blob.git"
  },
