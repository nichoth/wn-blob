{
  "name": "@nichoth/wn-blob",
  "description": "Save blobs to webnative",
  "directories": {
    "test": "test"
  },
  "type": "module",
  "version": "0.0.0",
  "main": "dist/index.js",
  "browser": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "tsc -w",
    "prebuild": "rm -rf dist/*",
    "build": "tsc",
    "test": "npm run build-test && cat test/index.html | tape-run --input html --static test",
    "build-test": "browserify -d -p esmify test/index.js > test/test-bundle.js"
  },
  "testling": {
    "browsers": [
      "chrome/latest"
    ],
    "files": "test/*.js"
  },
  "devDependencies": {
    "@ipld/dag-pb": "^2.1.13",
    "blockstore-core": "^1.0.2",
    "browserify": "^17.0.0",
    "datastore-core": "^6.0.7",
    "envify": "^4.1.0",
    "esmify": "github:nichoth/esmify",
    "ipfs-core": "^0.11.1",
    "ipfs-repo": "^13.0.5",
    "ipfs-utils": "^9.0.2",
    "localforage": "^1.10.0",
    "tap-spec": "^5.0.0",
    "tape": "^5.3.1",
    "tape-run": "github:nichoth/tape-run",
    "testling": "^1.7.3",
    "webnative": "^0.29.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nichoth/wn-blob.git"
  },
  "author": "Nick Thomas <nichoth@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nichoth/wn-blob/issues"
  },
  "homepage": "https://github.com/nichoth/wn-blob#readme",
  "dependencies": {
    "one-webcrypto": "github:nichoth/one-webcrypto",
    "peer-id": "^0.15.3"
  }
}
